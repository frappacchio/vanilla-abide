{
  "name": "vanilla-abide",
  "version": "1.2.4",
  "description": "A VanillaJS translation for Foundation Abide Form Validation",
  "main": "build/abide.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack-dev.js",
    "build": "webpack --config webpack-prod.js",
    "lint": "node_modules/.bin/eslint ./src",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frappacchio/vanilla-abide.git"
  },
  "keywords": [
    "vanillajs",
    "form",
    "validation",
    "abide",
    "foundation",
    "form-validation",
    "vanilla",
    "accessibility",
    "validation-library",
    "wcag",
    "foundation 6"
  ],
  "author": {
    "name": "frappacchio",
    "url": "https://github.com/frappacchio/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frappacchio/vanilla-abide/issues"
  },
  "homepage": "https://github.com/frappacchio/vanilla-abide#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^24.9.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/sample/**",
      "!**/jest-coverage-report/**",
      "!webpack.*.js"
    ],
    "coverageReporters": [
      "json-summary"
    ]
  }
}
